        "postcss": "^8.4.21",
        "prettier": "^2.7.1",
        "prettier-plugin-tailwindcss": "^0.1.13",
        "prisma": "^4.11.0",
        "readline-sync": "^1.4.10",
        "tailwindcss": "^3.2.7",
        "uuid": "^9.0.0"
    }
}
